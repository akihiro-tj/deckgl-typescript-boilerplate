{
  "name": "react-typescript-boilerplate",
  "version": "0.1.0",
  "repository": {
    "type": "git",
    "url": "https://github.com/akihiro-tj/react-typescript-boilerplate.git"
  },
  "private": true,
  "scripts": {
    "dev": "yarn start",
    "start": "craco start",
    "build": "craco build",
    "lint": "run-s lint:*",
    "lint:eslint": "eslint src",
    "lint:stylelint": "stylelint src/**/*.scss",
    "fix": "run-s fix:*",
    "fix:eslint": "eslint src --fix",
    "fix:stylelint": "stylelint src/**/*.scss --fix",
    "fix:prettify": "prettier --write *.{md,json}",
    "latest": "yarn upgrade-interactive --latest"
  },
  "dependencies": {
    "classnames": "^2.3.1",
    "react": "^17.0.2",
    "react-app-polyfill": "^2.0.0",
    "react-dom": "^17.0.2",
    "sanitize.css": "^13.0.0"
  },
  "devDependencies": {
    "@craco/craco": "^6.4.0",
    "@types/classnames": "^2.3.1",
    "@types/node": "^16.11.6",
    "@types/react": "^17.0.13",
    "@types/react-dom": "^17.0.8",
    "@typescript-eslint/eslint-plugin": "^4.29.2",
    "@typescript-eslint/parser": "^4.29.2",
    "autoprefixer": "^8",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-prettier": "^3.4.0",
    "eslint-plugin-react": "^7.24.0",
    "eslint-plugin-react-hooks": "^4.2.0",
    "fs-extra": "^10.0.0",
    "node-sass": "^4",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.3.11",
    "prettier": "^2.3.2",
    "react-scripts": "^4.0.0",
    "stylelint": "^14.0.0",
    "stylelint-config-recess-order": "^2.5.0",
    "stylelint-config-recommended": "^6.0.0",
    "stylelint-config-standard": "^23.0.0",
    "stylelint-scss": "^4.0.0",
    "typescript": "^4.0.5",
    "web-vitals": "^0.2.4"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "license": "MIT"
}
